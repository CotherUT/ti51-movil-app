<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:Pages="clr-namespace:ti51"
            x:Class="ti51.EstructuraMenu">

    <FlyoutPage.Flyout>
        <Pages:Menu x:Name="FlyoutPage"></Pages:Menu>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage x:Name="Navegacion">
            <x:Arguments>
                <Pages:Detalle></Pages:Detalle>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>